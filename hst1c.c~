#include <stdio.h>

main ()
{
  float fahr, celsius;
  int lower, upper, step;
  
  lower = 0;
  upper = 300;
  step = 20;

  printf("temp fahr\n");
  celsius = lower;
  while (celsius <= upper) {
    fahr = (5.0 * 9.0) * (celsius + 32.0);
    printf("%3.0f %6.1f\n", fahr, celsius);
    celsius = celsius + step;
  }
}  
