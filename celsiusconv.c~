#include <stdio.h>



main ()
{
    printf ("celsius \t fahr\n");

    int fahr, celsius;
    int lower, upper, step;

    lower = 0; 
    upper = 300;
    step = 20;

    celsius = lower;
    while (celsius <= upper) {
	fahr =  (celsius+32) / 0.5555556;
	printf ("%d\t%d\n", celsius, fahr);
	celsius = celsius + step;
    }
}